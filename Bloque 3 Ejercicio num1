let n = parseInt(prompt("Ingrese el numero de notas:"));
let suma = 0;
let contador = 0;
for (let i = 1; i < n; i++) {
    let nota = parseInt(prompt(`Ingrese la nota ${i + 1}:`));
    if (nota > 70) {
        suma += nota;
        contador++;
    }
}
if (contador > 0) {
    let promedio = suma / contador;
    console.log("El promedio de notas mayores a 70 es: " + promedio);
} else {
    console.log("No hay notas mayores a 70 para calcular el promedio.");
}
